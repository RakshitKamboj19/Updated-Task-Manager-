{
  "version": 2,
  "functions": {
    "api/task.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 86400
    }
  },
  "builds": [
    {
      "src": "backend/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app.js"
    }
  ]
}
